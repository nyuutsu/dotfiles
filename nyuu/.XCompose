include "%L"

## basic

# 😊 moods 😊
<Multi_key> <less> <3> : "♥" U2665 # BLACK HEART SUIT
<Multi_key> <colon> <parenleft> : "☻" U263B # BLACK SMILING FACE

# 🗳️ votes 🗳️
<Multi_key> <bracketleft> <bracketright> : "☑" U2611 # BALLOT BOX WITH CHECK
<Multi_key> <v> <v> : "✓" U2713 # CHECK MARK
<Multi_key> <X> <X> : "✗" U2717 # BALLOT X

# symbols
<Multi_key> <a> <t> <o> <m> : "⚛" U269B # ATOM SYMBOL
<Multi_key> <z> <a> <p> : "⚡" U26A1 # HIGH VOLTAGE SIGN 
<Multi_key> <r> <o> <d> : "⚕" U2695 # STAFF OF AESCULAPIUS
<Multi_key> <a> <t> : "⌘" U2318 # PLACE OF INTEREST SIGN
<Multi_key> <d> <C> : "℃" U2103 # DEGREE CELSIUS
<Multi_key> <d> <F> : "℉" U2109 # DEGREE FAHRENHEIT

# 🗺️ directions 🗺️
<Multi_key> <caret> <bar> : "↑" U2191 # UPWARDS ARROW
<Multi_key> <v> <bar> : "↓" U2193 # DOWNWARDS ARROW
<Multi_key> <less> <minus> : "←" U2190 # LEFTWARDS ARROW
<Multi_key> <minus> <greater> : "→" U2192 # RIGHTWARDS ARROW

# puncts
<Multi_key> <asterisk> <minus> : "•" U2022 # BULLET
<Multi_key> <period> <period> <period> : "…" U2026 # HORIZONTAL ELLIPSIS
<Multi_key> <asterisk> <asterisk> :"⁑" U2051 # TWO ASTERISKS ALIGNED VERTICALLY
<Multi_key> <asterisk> <asterisk> <asterisk> : "⁂" U2042 # ASTERISM
<Multi_key> <bar> <bar> : "¦" U00A6 # BROKEN BAR
<Multi_key> <period> <colon> : "∴" U2234 # THEREFORE
<Multi_key> <less> <less> : "「" U300C # LEFT CORNER BRACKET
<Multi_key> <greater> <greater> : "」" U300D # RIGHT CORNER BRACKET
<Multi_key> <slash> <slash> : "⧸" U29F8 # BIG SOLIDUS

# select emoji
<Multi_key> <t> <e> <a> : "☕" U2615 # HOT BEVERAGE
<Multi_key> <b> <a> <g> : "💰" U1F4B0 # MONEY BAG

# 📝 letters 📝
<Multi_key> <g> <D> : "Δ" U0394 # GREEK CAPITAL LETTER DELTA
<Multi_key> <g> <X> : "Ξ" U039e # GREEK CAPITAL LETTER XI
<Multi_key> <g> <l> : "λ" U03bb # GREEK SMALL LETTER LAMBDA


