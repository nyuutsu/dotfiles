URxvt.font: \
  xft:FiraCode Nerd Font Mono:style=regular:pixelsize=18, \
  xft:IPAmjMincho:style=regular,pixelsize=18

URxvt.colorIT:      #87af5f
URxvt.colorBD:      #d7d7d7
URxvt.colorUL:      #87afd7

URxvt.letterspace: 1
URxvt.scrollBar: false
URxvt.transparent: true
URxvt.shading: 35
! urxvt*blurRadius: 15

URxvt.perl-ext-common:  ...,resize-font,...
URxvt.internalBorder: 20px
URxvt.iconfile:  /usr/share/icons/Adwaita/scalable/apps/utilities-terminal-symbolic.svg
vim*transparent: false
vim.internalBorder: 0